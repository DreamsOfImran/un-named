{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "isLive": {
      "type": "boolean"
    },
    "description": {
      "type": "richtext"
    },
    "settings": {
      "type": "json",
      "private": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profiles"
    },
    "event": {
      "model": "event"
    },
    "user_media": {
      "model": "user-media",
      "via": "profile"
    },
    "votes": {
      "type": "integer",
      "default": 0
    }
  }
}
