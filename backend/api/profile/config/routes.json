{
  "routes": [
    {
      "method": "GET",
      "path": "/profiles",
      "handler": "profile.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/profiles/count",
      "handler": "profile.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/profiles/my",
      "handler": "profile.findMine",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/profiles/:id",
      "handler": "profile.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/profiles/:slug",
      "handler": "profile.create",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/order/:slug/:id",
      "handler": "profile.order",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/profiles/:id",
      "handler": "profile.update",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/profiles/:id",
      "handler": "profile.delete",
      "config": {
        "policies": ["global::isAuthenticated"]
      }
    }
  ]
}
